digraph {
	Start [label=Start]
	Whitespace [label=Whitespace]
	"Single Character Token" [label="Single Character Token"]
	"Identifier/Keyword" [label="Identifier/Keyword"]
	Number [label=Number]
	Error [label=Error]
	End [label=End]
	Start -> Whitespace [label=whitespace]
	Start -> "Single Character Token" [label="()+-*/=<>{};"]
	Start -> "Identifier/Keyword" [label="alphabetic or _"]
	Start -> Number [label="digit or . and digit"]
	Start -> Error [label="."]
	Whitespace -> Start [label="skip whitespace"]
	"Single Character Token" -> Start [label="add token"]
	"Identifier/Keyword" -> Start [label="add identifier/keyword token"]
	Number -> Start [label="add number token"]
	Error -> Start [label="add error token"]
	Error -> Number [label=digit]
}
